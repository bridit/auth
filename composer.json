{
  "name": "bridit/auth",
  "description": "Brid-IT Modular Micro-Framework (Auth)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "async-aws/ssm": "^1.0",
    "bridit/core": "^1.4",
    "bridit/database": "^1.1",
    "bridit/http": "^1.3",
    "bridit/support": "^1.0",
    "lcobucci/jwt": "^4.1",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "autoload": {
    "psr-4": {
      "Brid\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
